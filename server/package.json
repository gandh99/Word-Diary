{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for word diary",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "authServer": "nodemon authServer.js",
    "client": "cd ../client/ && npm start",
    "dev": "concurrently \"npm run server\" \"npm run authServer\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run build"
  },
  "author": "Gan Ding Han",
  "license": "ISC",
  "dependencies": {
    "@vitalets/google-translate-api": "^3.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "passport-local": "^1.0.0",
    "socket.io": "^2.3.0"
  }
}
